{
  "name": "certification_mails",
  "version": "2.1.3",
  "description": "",
  "author": "",
  "private": true,
  "license": "UNLICENSED",
  "scripts": {
    "build": "nest build",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "start": "nest start",
    "start:dev": "nest start --watch",
    "start:debug": "nest start --debug --watch",
    "start:prod": "node dist/main",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./test/jest-e2e.json",
    "typeorm": "typeorm-ts-node-commonjs -d src/config/database.config.ts",
    "migration:generate": "npm run typeorm -- migration:generate",
    "migration:run": "npm run typeorm -- migration:run",
    "migration:revert": "npm run typeorm -- migration:revert",
    "clean:install": "rm -rf node_modules package-lock.json && npm cache clean --force && npm install --legacy-peer-deps",
    "docker:dev": "docker build --target development -t certification-mails-dev .",
    "docker:prod": "docker build --target production -t certification-mails-prod .",
    "docker:prune": "docker volume prune",
    "dev:up": "docker-compose -f docker-compose.dev.yml --env-file .env.development up -d",
    "dev:down": "docker-compose -f docker-compose.dev.yml --env-file .env.development down",
    "dev:logs": "docker-compose -f docker-compose.dev.yml --env-file .env.development logs -f",
    "dev:rebuild": "docker-compose -f docker-compose.dev.yml --env-file .env.development up --build -d",
    "dev:shell": "docker-compose -f docker-compose.dev.yml --env-file .env.development exec app sh",
    "dev:restart": "docker-compose -f docker-compose.dev.yml --env-file .env.development restart app",
    "dev:clean": "docker-compose -f docker-compose.dev.yml --env-file .env.development down -v --remove-orphans",
    "dev:status": "docker-compose -f docker-compose.dev.yml --env-file .env.development ps",
    "prod:build": "npm run docker:prod",
    "prod:up": "docker-compose --env-file .env up -d",
    "prod:down": "docker-compose --env-file .env down",
    "prod:logs": "docker-compose --env-file .env logs -f",
    "prod:rebuild": "docker-compose --env-file .env up --build -d",
    "prod:restart": "docker-compose --env-file .env restart app",
    "prod:clean": "docker-compose --env-file .env down -v --remove-orphans",
    "prod:status": "docker-compose --env-file .env ps",
    "prod:shell": "docker-compose --env-file .env exec app sh",
    "prod:run": "docker run --rm -p ${PORT:-3000}:3000 --env-file .env certification-mails-prod:latest",
    "prod:run:bg": "docker run -d -p ${PORT:-3000}:3000 --env-file .env --name certification-mails-prod certification-mails-prod:latest",
    "prod:stop": "docker stop certification-mails-prod && docker rm certification-mails-prod",
    "prod:deploy": "npm run prod:build && npm run prod:up"
  },
  "dependencies": {
    "@aws-sdk/client-s3": "3.800.0",
    "@nestjs/common": "^11.0.1",
    "@nestjs/config": "^4.0.2",
    "@nestjs/core": "^11.0.1",
    "@nestjs/jwt": "^11.0.0",
    "@nestjs/passport": "^11.0.5",
    "@nestjs/platform-express": "^11.1.6",
    "@nestjs/schedule": "^6.0.1",
    "@nestjs/swagger": "^11.2.0",
    "@nestjs/typeorm": "^11.0.0",
    "@sendgrid/mail": "^8.1.6",
    "@types/uuid": "^10.0.0",
    "bcryptjs": "^3.0.2",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.2",
    "dotenv": "^17.2.2",
    "exceljs": "^4.4.0",
    "multer": "^2.0.2",
    "mysql2": "^3.15.1",
    "papaparse": "^5.5.3",
    "passport": "^0.7.0",
    "passport-jwt": "^4.0.1",
    "passport-local": "^1.0.0",
    "puppeteer": "^24.22.3",
    "reflect-metadata": "^0.2.2",
    "rxjs": "^7.8.1",
    "swagger-ui-express": "^5.0.1",
    "typeorm": "^0.3.27",
    "uuid": "^13.0.0",
    "xlsx": "^0.18.5"
  },
  "devDependencies": {
    "@eslint/eslintrc": "^3.2.0",
    "@eslint/js": "^9.18.0",
    "@nestjs/cli": "^11.0.0",
    "@nestjs/schematics": "^11.0.0",
    "@nestjs/testing": "^11.0.1",
    "@types/bcryptjs": "^2.4.6",
    "@types/exceljs": "^0.5.3",
    "@types/express": "^5.0.0",
    "@types/jest": "^30.0.0",
    "@types/multer": "^2.0.0",
    "@types/node": "^22.10.7",
    "@types/papaparse": "^5.3.16",
    "@types/passport-jwt": "^4.0.1",
    "@types/passport-local": "^1.0.38",
    "@types/supertest": "^6.0.2",
    "eslint": "^9.18.0",
    "eslint-config-prettier": "^10.0.1",
    "eslint-plugin-prettier": "^5.2.2",
    "globals": "^16.0.0",
    "jest": "^30.0.0",
    "prettier": "^3.4.2",
    "source-map-support": "^0.5.21",
    "supertest": "^7.0.0",
    "ts-jest": "^29.2.5",
    "ts-loader": "^9.5.2",
    "ts-node": "^10.9.2",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^5.7.3",
    "typescript-eslint": "^8.20.0"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": "src",
    "testRegex": ".*\\.spec\\.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "collectCoverageFrom": [
      "**/*.(t|j)s"
    ],
    "coverageDirectory": "../coverage",
    "testEnvironment": "node",
    "moduleNameMapper": {
      "^src/(.*)$": "<rootDir>/$1",
      "^uuid$": "uuid"
    },
    "transformIgnorePatterns": [
      "node_modules/(?!(uuid)/)"
    ]
  },
  "overrides": {
    "@aws-sdk/client-s3": "3.800.0"
  }
}
